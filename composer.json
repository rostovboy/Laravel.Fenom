{
    "name": "rostovboy/laravel-fenom",
    "description": "Fenom Template Engine for Laravel 7",
    "keywords": ["laravel", "fenom", "template", "view"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "fenom/fenom": "2.*",
        "illuminate/support": "7.*",
        "illuminate/view": "7.*",
        "illuminate/config": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "satooshi/php-coveralls": "dev-master"
    },

    "autoload": {
        "psr-4": {
            "Rostovboy\\Fenom\\": "src/Fenom"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable"
}